@model IEnumerable<VADS.Models.TotoModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TotoName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TotoName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TotoId }) |
            @Html.ActionLink("Details", "Details", new { id=item.TotoId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TotoId })
        </td>
    </tr>
}

</table>
